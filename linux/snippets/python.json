	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }

	"Print Function": {
		"prefix": "pr",
		"body": [
			"print(f'$1')",
		],
		"description": "Print in console python"
	},

	"Main Function": {
		"prefix": "ifmain",
		"body": [
			"if __name__ == '__main__': ",
			"   $1",
		],
		"description": "Print in console python"
	}
}