  "todo-tree.tree.showScanModeButton": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "files.associations": {
    "*.html": "html"
  },
  "angular.experimental-ivy": true,
  "python.pythonPath": "/usr/local/bin/python3",
  "diffEditor.ignoreTrimWhitespace": false,
